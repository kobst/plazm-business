{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["google", "plugin:react/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"valid-jsdoc": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"max-len": [
			"warn",
			{
				"code": 120,
				"ignoreRegExpLiterals": true
			}
		],
		"react/prop-types": 0,
		"quote-props": "off",
		"comma-dangle": "off"
	}
}
